        <Label x:Name="LRUfaultPagesCount" Content="0" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="336,177,0,0" Height="28"/>
        <Label x:Name="label7" Content="OPT" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="211,236,0,0" Height="28"/>
        <Label x:Name="OPTfaultPagesCount" Content="0" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="336,236,0,0" Height="28"/>
